{
  "manifest_version": 3,
  "name": "leetpush",
  "description": "Commit to Github directly from Leetcode",
  "version": "1.0.0",
  "options_page": "options.html",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "build/background.js"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
