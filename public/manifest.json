{
  "manifest_version": 3,
  "name": "leetpush",
  "description": "Commit to Github directly from Leetcode",
  "version": "1.0.0",
  "options_page": "options.html",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "icons": {
    "64": "images/icon-64.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png",
    "512": "images/icon-512.png"
  }
}
